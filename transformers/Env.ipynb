    "/etc/docker/daemon.json:\n",
    "{\n",
    "    \"runtimes\": {\n",
    "        \"nvidia\": {\n",
    "            \"args\": [],\n",
    "            \"path\": \"nvidia-container-runtime\"\n",
    "        }\n",
    "    }\n",
    "}\n",
    "dokcer-compose.yml:\n",
    "  jupyter:\n",
    "    <<: [*service-share]\n",
    "    image: jupyter/tensorflow-notebook:python-3.11\n",
    "    deploy:\n",
    "     resources:\n",
    "       reservations:\n",
    "         devices:\n",
    "           - driver: nvidia\n",
    "             count: 1\n",
    "             capabilities: [gpu]\n",
    "    environment:\n",
    "      http_proxy: http://gost:18888\n",
    "      https_proxy: http://gost:18888\n",
    "    volumes:\n",
    "    - ./data/jupyter:/home/jovyan"
   ]
